<div id="container content">
    <div class="row">
        <div class="col-md-12">
            #if ($msg)
                <h2>$msg</h2>
            #end
        </div>
    </div>
    <br>
    <div class="row">
        <div class="col-md-12">
            #foreach ($point in $points)
                <strong>${point}<br></strong>
            #end
        </div>
    </div>
</div>
